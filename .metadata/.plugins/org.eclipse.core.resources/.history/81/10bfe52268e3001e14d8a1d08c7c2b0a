package com.sathya.springex;

public class Patient {

	private String name;
	private int age;
	private String Doctor;
	private  String Pharmcy;
	public Patient(String name, int age, String doctor, String pharmcy) {
		super();
		this.name = name;
		this.age = age;
		Doctor = doctor;
		Pharmcy = pharmcy;
	}
	
	public void detailsInfo() {
		
		System.out.println("Hey"+name+" Welcome to Sathya World....");
		System.out.println("Check your AGE..."+age+" to get updates");
		System.out.println("Check your Doctor..."+Doctor+" to get updates");
		System.out.println("Check your Pharmcy..."+Pharmcy+" to get updates");
		
		
		
	}
	
	
	
	}
	
	

